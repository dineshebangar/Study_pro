<script type="text/javascript">
  $(document).ready(function(){

    $('#form-submit').click(function(e){
      //ga('send-b', 'event-b', 'whitepaper-page-form', 'whitepaper-page-click', 'whitepaper-btn-dwld');
      var error_len = [];
      $('.register-form input:visible').each(function(){
        if($(this).val().length == 0 || $(this).val() == $(this).attr('placeholder'))
          error_len.push($(this).attr('id'));
      });

      if(error_len.length > 0)
      {
        $('.register-form .mktFormMsg').text("All fields are required.");
        e.preventDefault();
      }
      else if(!checkEmail($('.register-form #Email'))){
        $('.register-form .mktFormMsg').text("All fields are required.");
        e.preventDefault();
      }
      else
      {
        $('.thankyou-content-outside').css('display','block');
        $('.registration').css('display','none');
        formSubmit(document.getElementById("mktForm_1121"));
      }
    });
  });

  $('#form-submit-overlay').click(function(e){
    var error_len = [];
    $('.dwld-form input:visible').each(function(){

      if($(this).attr('type') == 'checkbox' && !$(this).is(':checked')){
        error_len.push('Evaluation Agreement');
      }
      else if($(this).val().length == 0 || $(this).val() == $(this).attr('placeholder'))
      {
        error_len.push($(this).attr('id') );
      }

    });
    $('.dwld-form #FirstName,.dwld-form #LastName, .dwld-form #Title, .dwld-form #Company,#Email').css('border','1px solid #CCCCCC');
    $('.dwld-form .check-terms').css('color','#444444');

    if(error_len.length > 0)
    {
      for(var err in error_len)
      {
        if(error_len[err] == "Evaluation Agreement")
          $('.check-terms').css('color','#FF0000');
        else
          $('.dwld-form #'+error_len[err]).css('border-color','#FF0000');
      }
      e.preventDefault();
    }
    else if(!checkEmail($('.dwld-form #Email'))){
      $('.dwld-form #Email').css('border-color','#FF0000');
      e.preventDefault();
    }
    else
    {
      $('.thankyou-content').css('display','block');
      $('.dwld-form,.wrap').css('display','none');

      formSubmit(document.getElementById("mktForm_1120"));
      if((navigator.userAgent.match(/MSIE 9.0/i) || navigator.userAgent.match(/MSIE 8.0/i)))
        window.open("http://mkto-m0015.com/940ZQO73300002H007s3L01");
      else
        window.open("http://mkto-m0015.com/940ZQO73300002H007s3L01","download-form-sdk");
    }
  });


  function formSubmit(elt) {
    return Mkto.formSubmit(elt);
  }
  function formReset(elt) {
    return Mkto.formReset(elt);
  }
</script>