}

#container {
    margin:0 auto;
    width:1024px;
    color:#000;
    background:#FFFFFF;
    min-height: 100%;
    position: relative;
}
.header {
    background: none repeat scroll 0 0 transparent;
    clear: both;
    left: 0;
    overflow: hidden;
    position: absolute;
    top: 35px;
    z-index: 9;
    width: 100%;
}
