}


.main .ui-corner-all, .main .ui-corner-bottom,.main .ui-corner-right,.main .ui-corner-br{
    border-radius: 0;
}
.ui-tabs .ui-tabs-nav li.ui-tabs-active a, .ui-tabs .ui-tabs-nav li.ui-state-disabled a, .ui-tabs .ui-tabs-nav li.ui-tabs-loading a{
    background: none repeat scroll 0 0 #B6B6B4;
}

#izot-tabs.ui-tabs .ui-tabs-nav li.ui-tabs-active a, .ui-tabs .ui-tabs-nav li.ui-state-disabled a, .ui-tabs .ui-tabs-nav li.ui-tabs-loading a {
    background: none repeat scroll 0 0 #FEBD3A;
    color: #404040;
}
