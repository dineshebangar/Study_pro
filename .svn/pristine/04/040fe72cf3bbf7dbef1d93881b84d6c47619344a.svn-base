    color: #A6A6A6;
    display: inline-block;
    font-family: baufra-bold;
    font-size: 19px;
    width: 95%;
}
.main .ui-widget-header {
    background: none repeat scroll 0 0 #E6E6E6;
    border: medium none;
    width: 230px;
    padding: 0 !important;
    overflow: hidden;
    min-height: 400px;
}

.main #izot-tabs .ui-widget-header{
    background: none;
    border: medium none;
    width: 100%;
    padding: 0 !important;
    overflow: hidden;
    min-height: 0;
}
.main .ui-state-default,.main  .ui-widget-content .main  .ui-state-default,.main  .ui-widget-header .main .ui-state-default{
    border-radius: 0;
}
.main .ui-tabs-vertical .ui-tabs-nav li{
    border: 0px !important;
    margin: 0 !important;
}
.main li.ui-state-default{
    margin-bottom: 20px !important;
    padding: 0px !important;
}

.ui-tabs .ui-tabs-panel h2 {
    color: #666666;
    font-size: 19px;
    font-weight: bold;
    padding: 0 0 10px;
}
.main .ui-tabs-anchor {
    display: inline-block;
    padding: 15px !important;
    width: 88%;
}
.main .ui-tabs-vertical .ui-tabs-panel {
    float: left;
    margin-left: 0;
    padding: 32px 0 0 32px;
    width: 72%;
}
